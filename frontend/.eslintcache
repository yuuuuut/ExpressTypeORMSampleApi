ng] = useState(false)\n\n  /**\n   * Tokenを更新する。\n   */\n  const refreshToken = async () => {\n    const currentUser = firebase.auth().currentUser\n    if (!currentUser) {\n      console.log('None CurrentUser')\n      return\n    }\n\n    const token = await currentUser.getIdToken()\n    console.log(token)\n    if (!token) {\n      console.log('None Token')\n      return\n    }\n\n    localStorage.setItem('@token', token)\n  }\n\n  /**\n   * GoogleAuth SignIn\n   */\n  const signin = useCallback(async () => {\n    const provider = new firebase.auth.GoogleAuthProvider()\n\n    await firebase\n      .auth()\n      .signInWithPopup(provider)\n      .then(async (data) => {\n        if (data.user) {\n          if (data.user.displayName && data.user.photoURL) {\n            const user: User = {\n              id: data.user.uid,\n              displayName: data.user.displayName,\n              photoURL: data.user.photoURL,\n            }\n\n            await create(user)\n\n            const currentUser = firebase.auth().currentUser\n            if (!currentUser) throw new Error()\n\n            const token = await currentUser.getIdToken()\n            if (token) {\n              localStorage.setItem('@token', token)\n            }\n          }\n        }\n      })\n      .catch((e) => {\n        setUser(null)\n        if (e.code === 'auth/popup-closed-by-user') {\n          return\n        }\n      })\n  }, [])\n\n  /**\n   * GoogleAuth SignOut\n   */\n  const signout = useCallback(async () => {\n    firebase\n      .auth()\n      .signOut()\n      .then(() => {\n        setUser(null)\n        localStorage.removeItem('@token')\n      })\n  }, [])\n\n  useEffect(() => {\n    firebase.auth().onAuthStateChanged(async (user) => {\n      setLoading(true)\n\n      if (user) {\n        const token = await user.getIdToken()\n\n        localStorage.setItem('@token', token)\n\n        if (user.displayName && user.photoURL) {\n          const userData: User = {\n            id: user.uid,\n            displayName: user.displayName,\n            photoURL: user.photoURL,\n          }\n          setUser(userData)\n        }\n      } else {\n        setUser(null)\n        localStorage.removeItem('@token')\n      }\n      setLoading(false)\n    })\n  }, [])\n\n  return { user, loading, refreshToken, signin, signout }\n}\n\nexport { useAuth, AuthProvider }\n","/usr/app/src/contexts/Common.ts",[],"/usr/app/src/apis/user.api.ts",[],"/usr/app/src/libs/firebase.ts",[],"/usr/app/src/apis/axios.ts",[],{"ruleId":"63","severity":1,"message":"64","line":3,"column":25,"nodeType":"65","messageId":"66","endLine":3,"endColumn":57},{"ruleId":"63","severity":1,"message":"64","line":4,"column":1,"nodeType":"67","messageId":"66","endLine":4,"endColumn":15},{"ruleId":"63","severity":1,"message":"64","line":4,"column":20,"nodeType":"65","messageId":"66","endLine":4,"endColumn":25},{"ruleId":"68","severity":1,"message":"69","line":12,"column":5,"nodeType":"70","messageId":"71","endLine":12,"endColumn":16},{"ruleId":"68","severity":1,"message":"69","line":16,"column":5,"nodeType":"70","messageId":"71","endLine":16,"endColumn":16},{"ruleId":"68","severity":1,"message":"69","line":18,"column":17,"nodeType":"70","messageId":"71","endLine":18,"endColumn":28},{"ruleId":"68","severity":1,"message":"69","line":30,"column":5,"nodeType":"70","messageId":"71","endLine":30,"endColumn":16},{"ruleId":"68","severity":1,"message":"69","line":42,"column":7,"nodeType":"70","messageId":"71","endLine":42,"endColumn":18},{"ruleId":"68","severity":1,"message":"69","line":47,"column":5,"nodeType":"70","messageId":"71","endLine":47,"endColumn":16},{"ruleId":"68","severity":1,"message":"69","line":49,"column":7,"nodeType":"70","messageId":"71","endLine":49,"endColumn":18},"@typescript-eslint/explicit-module-boundary-types","Missing return type on function.","ArrowFunctionExpression","missingReturnType","FunctionDeclaration","no-console","Unexpected console statement.","MemberExpression","unexpe
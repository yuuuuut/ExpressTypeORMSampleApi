{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "dist/js",
    "rootDir": "src",
    "strict": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", ".vscode"]
}